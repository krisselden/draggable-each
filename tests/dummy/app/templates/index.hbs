<p>
  This example illustrates nested lists, and the ability to drag between lists.
</p>

<div style="width: 800px; overflow: hidden; float: left">
  view: {{view.elementId}}
  {{view.childViews.length}} <br> {{view.childViews}}

  {{#view Ember.View}}
    nested view: {{view.elementId}}
    nested {{view.childViews.length}}<br>{{view.childViews}}
  {{#view Ember.View}}1{{/view}}

  {{#draggable-each context=lists}}
  <div style="width:311px">
    view.elementId:{{view.elementId}} (X)
    {{view.childViews.length}} <br>{{view.childViews}}
    <hr>
    {{#view Ember.View}}
      <h1>List {{id}}</h1>
      view:{{view.elementId}}
      {{view.childViews.length}} <br>{{view.childViews}}

      {{#draggable-each context=list handleSelector=".draggable-item-handle" itemWasMoved="itemWasMoved" itemView="apple" connectWith=".ember-drag-list"}}
        <div class="draggable-item-handle">X</div>
        view.elementId:{{view.elementId}} (X)
        {{apple-pie name=name}}
        parentView: {{view.parentView.elementId}}
        {{#pumpkin-pie}}
        {{id}} : {{name}} â€“ parentView: {{view.parentView.elementId}}
        {{/pumpkin-pie}}
      {{/draggable-each}}
    {{/view}}
    </div>
  {{/draggable-each}}
  {{/view}}
</div>
{{#view Ember.View}}
  viewId: {{view.elementId}}
  {{view.childViews.length}} <br> {{view.childViews}}
  {{#draggable-each context=otherList handleSelector=".draggable-item-handle" itemWasMoved="itemWasMoved" itemView="apple" connectWith=".ember-drag-list"}}
  {{/draggable-each}}
{{/view}}

